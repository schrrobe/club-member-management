  "main": "dist/server.js",
  "scripts": {
    "dev": "tsc -w --preserveWatchOutput & nodemon --watch dist dist/server.js",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "node --enable-source-maps dist/server.js",
    "clean": "rm -rf dist",
    "prisma": "prisma",
    "db:generate": "prisma generate --config=./prisma.config.ts",
    "db:migrate": "prisma migrate dev --config=./prisma.config.ts",
    "db:push": "prisma db push --config=./prisma.config.ts",
    "db:studio": "prisma studio --config=./prisma.config.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
