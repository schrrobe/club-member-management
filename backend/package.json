    "@types/helmet": "^4.0.0",
    "@types/node": "22.18.13",
    "nodemon": "^3.1.11",
    "prisma": "7.1.0",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "17.2.3",
    "express": "5.2.1",
