{
  "name": "club-member-mangement",
  "private": true,
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f postgres",
    "dev": "pnpm -C backend dev",
    "build": "pnpm -C backend build",
    "typecheck": "pnpm -C backend typecheck",
    "start": "pnpm -C backend start",
    "clean": "pnpm -C backend clean",
    "lint": "eslint \"backend/src/**/*.ts\"",
    "lint:fix:backend": "eslint \"backend/src/**/*.ts\" --fix",
    "format:backend": "prettier -w \"backend/src/**/*.ts\"",
    "format:check:backend": "prettier -c \"backend/src/**/*.ts\""
  },
  "pnpm": {
    "onlyBuiltDependencies": ["prisma", "@prisma/engines"]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "prettier": "^3.7.4",
    "typescript": "5.8.3"
  }
}
